meme_dict = {                                                          #словарь               
            "КРИНЖ": "Что-то очень странное или стыдное",              #ключ то что в него вложено              
            "ЛОЛ": "Что-то очень смешное",                             #ключ то что в него вложено
            "РОФЛ": "шутка",                                           #ключ то что в него вложено
            "ЩИЩ": "одобрение или восторг"                             #ключ то что в него вложено
            }
    
word = input("Введите непонятное слово (большими буквами!): ")         #команда которая запрашивает слово и выводит то что в кавычках
    
                                                                 
if word in meme_dict.keys():                                           #команда сравнивает слова с тем ключами в словаре  
    print(meme_dict[word])                                             #команда выводит вложеный элемент из ключа 
else:                                                                  #иначе
    print('Такого слова нет')                                          #команда которая выводит то что в кавычках
